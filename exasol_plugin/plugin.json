                "displayHint": "my.exasoldb.host or IP",
                "displayOrder": 1
            },
            {
                "key": "port",
                "displayName": "Port",
                "displayHint": "9563",
                "displayOrder": 2
            },
            {
                "key": "username",
                "displayName": "User",
                "displayHint": "sys",
                "displayOrder": 3
            },
            {
                "key": "password",
                "displayName": "Password",
                "displayHint": "exasol",
                "displayOrder": 4
            }
        ]
    },
